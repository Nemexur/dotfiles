{
    "title": "Home Row Numbers",
    "rules": [
        {
            "description": "Spacebar + home row to numbers",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "a",
                        "modifiers": {
                            "optional": [
                                "spacebar"
                            ]
                        },
                    },
                    "to": [
                        {
                            "key_code": "1"
                        }
                    ]
                }
            ]
        }
    ]
}
